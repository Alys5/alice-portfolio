{
  "extends": "./tsconfig.app.json",
  "include": ["src/**/__tests__/*", "env.d.ts"],
  "exclude": [],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.vitest.tsbuildinfo",
    "incremental": true,
    "skipLibCheck": true,
    "types": ["node", "jsdom"],
    "paths": {
      "@/*": ["./src/*"]
    },
    "target": "ES2015",
    "lib": ["ES2015", "DOM"]
  }
}
